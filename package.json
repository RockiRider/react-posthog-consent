{
  "name": "vite-plugin-posthog",
  "private": true,
  "scripts": {
    "dev": "turbo dev --filter=example-react",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "build": "turbo build --filter=react-posthog-consent",
    "example:build": "turbo build --filter=example-react",
    "deploy": "bash ./bin/deploy.sh react-posthog-consent"
    
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/node": "^20.11.0",
    "prettier": "^3.1.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@8.19.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
